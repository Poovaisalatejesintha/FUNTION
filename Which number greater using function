//without return type,without parameter
#include<stdio.h>
void num();
void main()
{
    num();
}
void num()
{
    int a,b;
    printf("Enter the numbers:");
    scanf("%d%d",&a,&b);
    if(a>b)
    {
        printf("\nThe number %d is greater",a);
    }
    else
    {
        printf("\nThe number %d is greater",b);
    }
}

***************************************************************************************

//without return type,with parameter
#include<stdio.h>
void num(int,int);
void main()
{
    int a,b;
    printf("Enter the numbers:");
    scanf("%d%d",&a,&b);
    num(a,b);
}
void num(int a,int b)
{
    
    if(a>b)
    {
        printf("\nThe number %d is greater",a);
    }
    else
    {
        printf("\nThe number %d is greater",b);
    }
}
*******************************************************************************************

//with return type,without parameter
#include<stdio.h>
int num();
int a,b;
void main()
{
    int ans;
    ans=num();
    if(ans==1)
    {
        printf("\nThe number %d is greater",a);
    }
    else
    {
        printf("\nThe number %d is greater",b);
    }
}
int num()
{
    printf("Enter the numbers");
    scanf("%d%d",&a,&b);
    if(a>b)
    {
        return 1;
    }
    else
    {
        return 0;
    }
}
****************************************************************************************************

//with return type,with parameter
#include<stdio.h>
int num(int,int);
int a,b;
void main()
{
    int ans;
    printf("Enter the numbers");
    scanf("%d%d",&a,&b);
    ans=num(a,b);
    if(ans==1)
    {
        printf("\nThe number %d is greater",a);
    }
    else
    {
        printf("\nThe number %d is greater",b);
    }
}
int num(int a,int b)
{
    if(a>b)
    {
        return 1;
    }
    else
    {
        return 0;
    }
}
*******************************************************************************************


